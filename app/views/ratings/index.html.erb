<p style="color: green"><%= notice %></p>

<h2>List of ratings</h2>
<p>Total ratings: <%= Rating.count %></p>

<ul>
  <% @ratings.each do |rating| %>
    <li>
      <%= rating.to_s %>
      <%= link_to rating.user.username, rating.user %>
    </li>
  <% end %>
</ul>

<p>
  <%= link_to "New rating", new_rating_path %>
</p>